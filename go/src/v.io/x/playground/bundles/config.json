{
  "examples": [
    {
      "name": "fortune",
      "path": "fortune",
      "globs": [
        "go",
        "js",
        "js-go",
        "go-js"
      ],
      "output": [
        "server.*Serving:",
        "client.*Received:",
        "client.*Exited cleanly\\."
      ]
    }
  ],
  "globs": {
    "go": {
      "patterns": [
        "*.vdl",
        "client/**/*.go",
        "server/**/*.go"
      ]
    },
    "js": {
      "patterns": [
        "*.vdl",
        "client/**/*.js",
        "server/**/*.js"
      ]
    },
    "js-go": {
      "patterns": [
        "*.vdl",
        "client/**/*.js",
        "server/**/*.go"
      ]
    },
    "go-js": {
      "patterns": [
        "*.vdl",
        "client/**/*.go",
        "server/**/*.js"
      ]
    }
  }
}
